{
  "name": "demo",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --color --hot --progress --content-base public/",
    "test": "cross-env NODE_ENV=test nyc --all mocha --opts mocha.opts --no-deprecation",
    "test:watch": "npm run test -- --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "eslint": "eslint src --ext js --fix; exit 0",
    "stylelint": "stylelint '**/*.scss'; exit 0"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "build",
      "dist",
      "src/index.js"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.32",
    "@babel/preset-env": "7.0.0-beta.32",
    "@babel/preset-react": "7.0.0-beta.32",
